// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.vs/": true,
        "node_modules/": true,
        "**/bin/*/net*": true,
        "**/obj/": true,
        "packages/": true,
        "fable": true
    },
    "editor.rulers": [
        {
            "column": 90,
            "color": "#7779"
        }
    ],
    "[markdown]": {
        "editor.rulers": [
            {
                "column": 90,
                "color": "#7779"
            },        {
                "column": 39,
                "color": "#7773"
            },
            {
                "column": 46,
                "color": "#7773"
            }
        ]
    }
}
